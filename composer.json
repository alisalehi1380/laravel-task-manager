{
  "name": "alisalehi/laravel-task-manager",
  "description": "for manage task in laravel",
  "keywords": [
    "laravel task manager package",
    "laravel task manager"
  ],
  "homepage": "https://github.com/alisalehi1380/laravel-task-manager",
  "license": "MIT",
  "authors": [
    {
      "name": ":Ali Salehi",
      "email": "alisalehi13800930@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "9.*|10.*"
  },
  "autoload": {
    "psr-4": {
      "AliSalehi\\Task\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AliSalehi\\Task\\TaskServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^8.22",
    "phpunit/phpunit": "^10.5"
  },
  "autoload-dev": {
    "psr-4": {
      "AliSalehi\\Task\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/BaseTest.php"
  }
}